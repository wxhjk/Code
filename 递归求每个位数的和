import java.util.*;
public class text {
    public static void main(String[] args) {
     Scanner sz=new Scanner(System.in);
     int n=sz.nextInt();
     System.out.println(sumAdd(n));
    }
    public static int sumAdd(int n){
     if(n<=9){
         return n;
     }
     else{
         return sumAdd(n/10)+n%10;
     }
    }
}