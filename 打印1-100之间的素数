int i=0;
         for(i=1;i<=100;i++){
             int j=0;
             int b=0;
             for(j=2;j<=i/2;j++){
                 if(i%j==0){
                     b+=1;
                     break;
                 }
             }
             if(b==0){
                 System.out.println(i);
             }
         }