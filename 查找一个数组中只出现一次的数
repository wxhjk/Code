public class text {
    public static void main(String[] args) {
      int[] arr={1,2,3,4,2,3,4,5,6,1,6};
        for (int i = 0; i < arr.length; i++) {
            int j=0;
            for ( j = 0; j < arr.length; j++) {
                if(arr[i]==arr[j]&&i!=j){
                    break;
                }
            }
            if(j==arr.length){
                System.out.println(arr[i]);
                break;
            }
        }
    }
}