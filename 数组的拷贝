public class text {
   public static void main(String[] args) {
      int[] arr={1,2,3,5};
      int[] ret=copy(arr);
       System.out.println(Arrays.toString(ret));
   }
   public static int[] copy(int[] arr){
       int[] ret=new int[arr.length];
       for (int i = 0; i <arr.length ; i++) {
           ret[i]=arr[i];
       }
       return ret;
   }
}
