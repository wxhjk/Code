 Scanner sz=new Scanner(System.in);
     while(true){
         int a=sz.nextInt();
         int i=0;
         int b=0;
         for(i=2;i<=a/2;i++){
             if(a%i==0){
                 b+=1;
                 break;
             }
         }
         if(b==0){
             System.out.println("是素数");
         }
         else{
             System.out.println("不是素数");
         }
     }