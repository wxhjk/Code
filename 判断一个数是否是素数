 Scanner sz =new Scanner(System.in);
     while(sz.hasNextInt()){
         int n=sz.nextInt();
         int i=0;
         for (i = 2; i <=Math.sqrt(n) ; i++) {
             if(n%i==0){
                 System.out.println("不是素数");
                 break;
             }
         }
         if(i>Math.sqrt(n)){
             System.out.println("是素数");
         }
     }