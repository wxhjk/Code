public class text {
   public static void main(String[] args) {
      int[] arr={1,2,3,4,5};
      int[] ret= transform(arr);
       System.out.println(Arrays.toString(ret));
  }
  public static int[] transform(int[] arr){
      int[] ret=new int[arr.length];
      for (int i = 0; i < arr.length; i++) {
          ret[i]=arr[i]*2;
      }
      return ret;
  }
}