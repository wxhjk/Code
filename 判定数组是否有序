public class text {
   public static void main(String[] args) {
    int[] arr={1,2,3,4,5,6,5};
       System.out.println(judge(arr));
   }
   public static int judge(int[] arr){
       for (int i = 0; i <arr.length-1 ; i++) {
           if (arr[i]>arr[i+1]){
               return -1; 

           }
       }
       return 0;
   }
}
