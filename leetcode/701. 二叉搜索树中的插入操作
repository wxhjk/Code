class Solution {
    public TreeNode insertIntoBST(TreeNode root, int val) {
        if (root == null) {
            return new TreeNode(val);
        }
        TreeNode cur = root;
        TreeNode pre = null;
        while (cur != null) {
            pre = cur;
            if (cur.val > val) {
                cur = cur.left;
            }else {
                cur = cur.right;
            }
        }
        TreeNode node = new TreeNode(val);
        if (pre.val < val) {
            pre.right = node;
        }else {
            pre.left = node;
        }
        return root;
    }
}