class Solution {
    public String longestCommonPrefix(String[] strs) {
        if(strs.length==0){
            return "";
        }
        String ret=strs[0];
        for(int i=1;i<strs.length;i++){
            int j=0;
            while(j<ret.length()&&j<strs[i].length()&&ret.charAt(j)==strs[i].charAt(j)){
                j++;
            }
            ret=ret.substring(0,j);
        }
        return ret;
    }
}