class Solution {
    public boolean backspaceCompare(String s, String t) {
         int p1 = 0;
         int p2 = 0;
         int i = s.length()-1;
         int j = t.length()-1;
         while (i >= 0 || j >= 0) {
             while (i >= 0){
                 if (s.charAt(i) == '#') {
                     p1++;
                     i--;
                 }else if (p1 > 0) {
                     p1--;
                     i--;
                 }else{
                     break;
                 }
             }
             while (j >= 0) {
                 if (t.charAt(j) == '#') {
                     p2++;
                     j--;
                 }else if (p2 > 0) {
                     p2--;
                     j--;
                 }else{
                     break;
                 }
             }
             if (i >= 0 && j >= 0){
                 if (s.charAt(i) != t.charAt(j)) {
                     return false;
                 }
             }else{
                 if (i >= 0 || j >= 0) {
                     return false;
                 } 
             }
             i--;
             j--;
         }
         return true;
    }
}