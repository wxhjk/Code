import java.util.*;
public class text {
    public static void main(String[] args) {
     Scanner sz=new Scanner(System.in);
     int n=sz.nextInt();
        System.out.println(ride(n));
    }
public static int ride(int n){
        if(n==1){
            return 1;
        }
        else{
            return n*ride(n-1);
        }
  }
}